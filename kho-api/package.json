{
    "name": "express-babel",
    "version": "1.0.0",
    "description": "Express app using babel with latest JavaScript features",
    "main": "dist/index.js",
    "repository": "https://github.com/hoangphison/express-babel.git",
    "scripts": {
        "clean:dist": "rimraf dist",
        "start": "nodemon --exec babel-node src/index.js",
        "start-demo": "nodemon --exec babel-node src/index.js --host 0.0.0.0 ",
        "build": "npm run clean:dist && babel src --out-dir dist",
        "serve": "node dist/index.js",
        "lint": "eslint src/*"
    },
    "keywords": [
        "express",
        "babel",
        "es6",
        "boilerplate",
        "scaffold"
    ],
    "author": "HOANG PHI SON <hello@hoangphison.com>",
    "license": "MIT",
    "dependencies": {
        "axios": "^0.18.0",
        "bcrypt-nodejs": "0.0.3",
        "body-parser": "^1.18.3",
        "camelcase": "^5.0.0",
        "cors": "^2.8.5",
        "dotenv": "^6.1.0",
        "express": "^4.16.4",
        "express-flash": "0.0.2",
        "express-session": "^1.15.6",
        "express-validator": "^5.3.0",
        "fs-extra": "^7.0.1",
        "http-status": "^1.2.0",
        "jsonwebtoken": "^8.3.0",
        "lodash": "^4.17.11",
        "moment": "^2.22.2",
        "mongoose": "^5.3.7",
        "mongoose-paginate-v2": "^1.0.12",
        "mongoose-unique-validator": "^2.0.2",
        "morgan": "^1.9.1",
        "multer": "^1.4.1",
        "passport": "^0.4.0",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "pusher": "^2.2.0",
        "pusher-js": "^4.3.1",
        "quickthumb": "0.0.12",
        "sharp": "^0.21.0",
        "split": "^1.0.1",
        "throw.js": "^3.0.1",
        "uuid": "^3.3.2",
        "winston": "^2.4.4",
        "winston-daily-rotate-file": "^3.4.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.1.2",
        "@babel/core": "^7.1.2",
        "@babel/node": "^7.0.0",
        "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
